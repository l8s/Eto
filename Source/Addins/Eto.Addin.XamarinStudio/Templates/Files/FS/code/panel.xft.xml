<?xml version="1.0"?>
<Template>
	<TemplateConfiguration>
		<_Name>Panel (with form preview)</_Name>
		<Icon>md-text-file-icon</Icon>
		<_Category>Eto.Forms</_Category>
		<LanguageName>F#</LanguageName>
		<_Description>
			<![CDATA[Creates a new cross platform Panel class using Eto.Forms in F#.]]>
		</_Description>
		<DefaultFilename>EtoPanel</DefaultFilename>
		<GroupId>eto-file-panel</GroupId>
	</TemplateConfiguration>
	<TemplateFiles>
        <File name="${Name}.fs" AddStandardHeader="true" src="panel.fs"/>
        <File name="${Name}.eto.fs" AddStandardHeader="true" src="panel.eto.fs"/>
	</TemplateFiles>
</Template>
